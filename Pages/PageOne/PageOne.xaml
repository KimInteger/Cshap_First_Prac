<Page x:Class="MoveSquare.Pages.PageOne"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="PageOne">
	<Grid x:Name="FirstMap">
		<TextBlock Text="Welcome to Page One!" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>

		<!-- PageTwo로 이동하는 버튼 추가 -->
		<Button Content="Go to Page Two"
                Width="200"
                Height="50"
                HorizontalAlignment="Center"
                VerticalAlignment="Bottom"
                Margin="0,20,0,0"
                Click="GoToPageTwo_Click"/>
	</Grid>
</Page>
