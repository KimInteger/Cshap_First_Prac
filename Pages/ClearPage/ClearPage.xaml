<Page x:Class="MoveSquare.Pages.ClearPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="ClearPage">
    <Grid>
        <TextBlock Text="You have reached the Clear Page!" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>

        <Button Content="Return to Main"
                Width="200"
                Height="50"
                HorizontalAlignment="Center"
                VerticalAlignment="Bottom"
                Margin="0,20,0,0"
                Click="ReturnToMain_Click"/>
    </Grid>
</Page>
